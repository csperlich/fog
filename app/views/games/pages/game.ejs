<!DOCTYPE html>
<html>
  <head>
    <% include ../../common/partials/head %>
    <link rel="stylesheet" href="/styles/forums.css">

    <% scripts.forEach(function(script) { %>
      <script type="text/javascript" src="<%= gamePath + script %>"></script>
    <% }); %>

    <% styles.forEach(function(style) { %>
      <link rel="stylesheet" href="<%= gamePath + style.replace(/ /, '%20') %>">
    <% }); %>

    <script type="text/javascript" src="/helpers/games/gameKickerOff.js"></script>
    <title>fog | <%= title %></title>
  </head>
  <body>

    <header>
      <% include ../../common/partials/header %>
    </header>

    <main>

      <div id="game" data-fn="<%= startingPoint %>" data-path="<%= gamePath %>">

      </div>


    </main>

    <footer>
      <% include ../../common/partials/footer %>
    </footer>
  </body>
</html>
